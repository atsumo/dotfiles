 {
  "Keymap": {
    "C-s": "peco.ToggleSelectionAndSelectNext",
    "C-a": "peco.SelectAll",
    "C-g": "peco.SelectNone",
    "C-u": "peco.ScrollPageUp",
    "C-d": "peco.ScrollPageDown",
    "C-f": "peco.ToggleSingleKeyJump",
    "Pgdn": "peco.ScrollPageUp",
    "Pgdn": "peco.ScrollPageDown",
    "C-j": "peco.SelectDown",
    "C-k": "peco.SelectUp",
    "C-f": "peco.ScrollPageDown",
    "C-b": "peco.ScrollPageUp",
    "Tab":"peco.RotateMatcher"
  },
    "CustomFilter": {
        "Migemo": {
            "Cmd": "migemogrep",
            "Args": [ "$QUERY" ]
        }
    }
}
