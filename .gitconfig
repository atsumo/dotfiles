[core]
	editor = vim
[include]
	path = .gitconfig.local
[color]
	ui = auto
[alias]
	co = checkout
	ci = commit
	st = status -sb
	b = branch
	d = diff
	dc = diff --cached
[user]
	name = atsumo
	email = bachisuta@gmail.com
[pager]
    log = diff-highlight | less
    show = diff-highlight | less
    diff = diff-highlight | less
[push]
  default = current
[ghq]
  root = ~/.ghq
  root = ~/src
