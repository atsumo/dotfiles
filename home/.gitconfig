[core]
	editor = vim
	excludesfile = /Users/a11673/.gitignore_global
[include]
	path = .gitconfig.local
[color]
	ui = auto
[alias]
	co = checkout
	ci = commit
	st = status -sb
	b = branch
	d = diff
	dc = diff --cached
[user]
	name = atsumo
	email = bachisuta@gmail.com
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /opt/homebrew-cask/Caskroom/sourcetree/1.9.7/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[pager]
    log = diff-highlight | less
    show = diff-highlight | less
    diff = diff-highlight | less
[push]
  default = current
[ghq]
  root = ~/.ghq
  root = ~/.go/src
